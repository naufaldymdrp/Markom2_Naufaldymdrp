@using Markom2.Repository.ViewModels
@model IList<VMEmployee>

@if (Model != null)
{
    @foreach (var modelItem in Model)
    {
        <tr>
            <td>@Html.DisplayFor(_ => modelItem.Id)</td>
            <td>@Html.DisplayFor(_ => modelItem.Code)</td>
            <td>@Html.DisplayFor(_ => modelItem.Name)</td>
            <td>@Html.DisplayFor(_ => modelItem.CompanyName)</td>
            <td>@Html.DisplayFor(_ => modelItem.CreatedDate)</td>
            <td>@Html.DisplayFor(_ => modelItem.CreatedBy)</td>
            <td>
                <span class="fas fa-search-plus memployee-detail-button" aria-hidden="true" id="@modelItem.Id"></span>
                <span class="fas fa-pen memployee-edit-button" aria-hidden="true" id="@modelItem.Id"></span>
                <span class="fas fa-trash memployee-delete-button" aria-hidden="true" id="@modelItem.Id"></span>
            </td>
        </tr>
    }
}